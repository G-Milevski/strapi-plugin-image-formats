{
  "routes": [
    {
      "method": "POST",
      "path": "/",
      "handler": "ImageFormats.create",
      "config": {
        "policies": ["global.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/",
      "handler": "ImageFormats.index",
      "config": {
        "policies": ["global.isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/:imageFormatName",
      "handler": "ImageFormats.delete",
      "config": {
        "policies": ["global.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/:imageFormatName/:fileId",
      "handler": "ImageFormats.getFormattedImage",
      "config": {
        "policies": []
      }
    }
  ]
}
